{
  "name": "encompass",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:app:dev": "yarn nx run encompass:serve --configuration=development",
    "start:api:dev": "yarn nx run api:serve --configuration=development",
    "build:app:dev": "yarn nx run encompass:build --configuration=development",
    "build:api:dev": "yarn nx run api:build --configuration=development",
    "build:prod": "yarn nx run api:build --configuration=production && yarn nx run encompass:build --configuration=production",
    "start:prod": "yarn nx run api:serve --configuration=deployment && yarn nx run encompass:serve --configuration=deployment",
    "test": "nx run-many --target=test --all --skip-nx-cache --parallel=5",
    "lint": "nx run-many --target=lint --all --skip-nx-cache --parallel=5"
  },
